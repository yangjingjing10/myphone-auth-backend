{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/admin.html",
      "dest": "admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}

